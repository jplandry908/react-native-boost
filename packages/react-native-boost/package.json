{
  "name": "react-native-boost",
  "description": "ðŸš€ Boost your React Native app's performance with a single line of code",
  "version": "0.0.5",
  "main": "dist/plugin.js",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "performance",
    "optimization",
    "bundle",
    "optimize"
  ],
  "scripts": {
    "clean": "rm -rf plugin",
    "build": "yarn clean && node scripts/esbuild.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "release": "release-it",
    "prepack": "cp ../../README.md ./README.md",
    "postpack": "rm ./README.md"
  },
  "files": [
    "src",
    "plugin",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuatsu/react-native-boost.git"
  },
  "author": "Kuatsu App Agency <hello@kuatsu.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuatsu/react-native-boost/issues"
  },
  "homepage": "https://github.com/kuatsu/react-native-boost#readme",
  "packageManager": "yarn@3.6.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@babel/core": "^7.25.0",
    "@babel/helper-module-imports": "^7.25.0",
    "@babel/helper-plugin-utils": "^7.25.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/babel__helper-module-imports": "^7.0.0",
    "@types/babel__helper-plugin-utils": "^7.0.0",
    "@types/node": "^20",
    "babel-plugin-tester": "^11.0.4",
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.18.0",
    "globals": "^16.0.0",
    "release-it": "^18.1.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        },
        "infile": "CHANGELOG.md"
      }
    }
  }
}
